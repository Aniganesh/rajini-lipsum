{
	"name": "rajini-lipsum",
	"displayName": "Rajini Lipsum",
	"description": "Lorem ipsum but it's Rajini's lines!",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rajini-lipsum.test",
		"onCommand:rajini-lipsum.lines"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rajini-lipsum.test",
				"title": "Test rajini lipsum"
			},
			{
				"command": "rajini-lipsum.lines",
				"title": "Rajini lipsum lines"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}